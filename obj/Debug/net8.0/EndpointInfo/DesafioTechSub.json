            "format": "int32"
          },
          "revenue": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "Subscription": {
        "required": [
          "chargeDate",
          "isCanceled",
          "subscriptionPlanId",
          "subscriptionValue",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "subscriptionPlanId": {
            "type": "string",
            "format": "uuid"
          },
          "subscriptionValue": {
            "type": "number",
            "format": "double"
          },
          "chargeDate": {
            "type": "string",
            "format": "date-time"
          },
          "isCanceled": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "SubscriptionPlan": {
        "required": [
          "allowTrial",
          "anualPrice",
          "monthlyPrice",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "monthlyPrice": {
            "type": "number",
            "format": "double"
          },
          "anualPrice": {
            "type": "number",
            "format": "double"
          },
          "allowTrial": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "email",
          "isSubscriptionActive",
          "isTrialExpired",
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "username": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "password": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "paymentMethodId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "isSubscriptionActive": {
            "type": "boolean"
          },
          "isTrialExpired": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "UserReport": {
        "type": "object",
        "properties": {
          "user": {
            "type": "string",
            "nullable": true
          },
          "subscriptionPlan": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
